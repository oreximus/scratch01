Commerce/src/pages/Checkout.jsx",["118"],[],"/home/addiarch/Downloads/React_E-Commerce/src/pages/PageNotFound.jsx",[],[],"/home/addiarch/Downloads/React_E-Commerce/src/redux/reducer/handleCart.js",["119","120","121"],[],"const cart = []\n\nconst handleCart = (state=cart, action) =>{\n    const product = action.payload\n    switch(action.type){\n        case \"ADDITEM\":\n            // Check if product already in cart\n            const exist = state.find((x) => x.id === product.id)\n            if(exist){\n                // Increase the quantity\n                return state.map((x)=>x.id ===product.id?{...x, qty: x.qty+1}:x)\n            }\n            else{\n                return [...state, {...product, qty:1}]\n            }\n            break;\n        case \"DELITEM\":\n            const exist2 = state.find((x) => x.id === product.id)\n            if(exist2.qty === 1){\n                return state.filter((x)=>x.id!==exist2.id)\n            }\n            else{\n                return state.map((x)=> x.id===product.id?{...x, qty:x.qty-1}:x)\n            }\n            break;\n\n        default:\n            return state\n            break;\n    }\n}\n\nexport default handleCart","/home/addiarch/Downloads/React_E-Commerce/src/components/index.js",[],[],"/home/addiarch/Downloads/React_E-Commerce/src/redux/action/index.js",[],[],"/home/addiarch/Downloads/React_E-Commerce/src/components/main.jsx",[],[],"/home/addiarch/Downloads/React_E-Commerce/src/components/Navbar.jsx",[],[],"/home/addiarch/Downloads/React_E-Commerce/src/components/Products.jsx",["122"],[],"import React, { useState, useEffect } from \"react\";\nimport { useDispatch } from \"react-redux\";\nimport { addCart } from \"../redux/action\";\n\nimport Skeleton from \"react-loading-skeleton\";\nimport \"react-loading-skeleton/dist/skeleton.css\";\n\nimport { Link } from \"react-router-dom\";\n\nconst Products = () => {\n  const [data, setData] = useState([]);\n  const [filter, setFilter] = useState(data);\n  const [loading, setLoading] = useState(false);\n  let componentMounted = true;\n\n  const dispatch = useDispatch();\n\n  const addProduct = (product) => {\n    dispatch(addCart(product));\n  };\n\n  useEffect(() => {\n    const getProducts = async () => {\n      setLoading(true);\n      const response = await fetch(\"https://fakestoreapi.com/products/\");\n      if (componentMounted) {\n        setData(await response.clone().json());\n        setFilter(await response.json());\n        setLoading(false);\n      }\n\n      return () => {\n        componentMounted = false;\n      };\n    };\n\n    getProducts();\n  }, []);\n\n  const Loading = () => {\n    return (\n      <>\n        <div className=\"col-12 py-5 text-center\">\n          <Skeleton height={40} width={560} />\n        </div>\n        <div className=\"col-md-4 col-sm-6 col-xs-8 col-12 mb-4\">\n          <Skeleton height={592} />\n        </div>\n        <div className=\"col-md-4 col-sm-6 col-xs-8 col-12 mb-4\">\n          <Skeleton height={592} />\n        </div>\n        <div className=\"col-md-4 col-sm-6 col-xs-8 col-12 mb-4\">\n          <Skeleton height={592} />\n        </div>\n        <div className=\"col-md-4 col-sm-6 col-xs-8 col-12 mb-4\">\n          <Skeleton height={592} />\n        </div>\n        <div className=\"col-md-4 col-sm-6 col-xs-8 col-12 mb-4\">\n          <Skeleton height={592} />\n        </div>\n        <div className=\"col-md-4 col-sm-6 col-xs-8 col-12 mb-4\">\n          <Skeleton height={592} />\n        </div>\n      </>\n    );\n  };\n\n  const filterProduct = (cat) => {\n    const updatedList = data.filter((item) => item.category === cat);\n    setFilter(updatedList);\n  };\n  const ShowProducts = () => {\n    return (\n      <>\n        <div className=\"buttons text-center py-5\">\n          <button\n            className=\"btn btn-outline-dark btn-sm m-2\"\n            onClick={() => setFilter(data)}\n          >\n            All\n          </button>\n          <button\n            className=\"btn btn-outline-dark btn-sm m-2\"\n            onClick={() => filterProduct(\"men's clothing\")}\n          >\n            Men's Clothing\n          </button>\n          <button\n            className=\"btn btn-outline-dark btn-sm m-2\"\n            onClick={() => filterProduct(\"women's clothing\")}\n          >\n            Women's Clothing\n          </button>\n          <button\n            className=\"btn btn-outline-dark btn-sm m-2\"\n            onClick={() => filterProduct(\"jewelery\")}\n          >\n            Jewelery\n          </button>\n          <button\n            className=\"btn btn-outline-dark btn-sm m-2\"\n            onClick={() => filterProduct(\"electronics\")}\n          >\n            Electronics\n          </button>\n        </div>\n\n        {filter\n          .slice(0)\n          .reverse()\n          .map((product) => {\n            return (\n              <div\n                id={product.id}\n                key={product.id}\n                className=\"col-md-4 col-sm-6 col-xs-8 col-12 mb-4\"\n              >\n                <div className=\"card text-center h-100\" key={product.id}>\n                  <img\n                    className=\"card-img-top p-3\"\n                    src={product.image}\n                    alt=\"Card\"\n                    height={300}\n                  />\n                  <div className=\"card-body\">\n                    <h5 className=\"card-title\">\n                      {product.title.substring(0, 12)}...\n                    </h5>\n                    <p className=\"card-text\">\n                      {product.description.substring(0, 90)}...\n                    </p>\n                  </div>\n                  <ul className=\"list-group list-group-flush\">\n                    <li className=\"list-group-item lead\">$ {product.price}</li>\n                    {/* <li className=\"list-group-item\">Dapibus ac facilisis in</li>\n                    <li className=\"list-group-item\">Vestibulum at eros</li> */}\n                  </ul>\n                  <div className=\"card-body\">\n                    <Link\n                      to={\"/product/\" + product.id}\n                      className=\"btn btn-dark m-1\"\n                    >\n                      Buy Now\n                    </Link>\n                    <button\n                      className=\"btn btn-dark m-1\"\n                      onClick={() => addProduct(product)}\n                    >\n                      Add to Cart\n                    </button>\n                  </div>\n                </div>\n              </div>\n            );\n          })}\n      </>\n    );\n  };\n  return (\n    <>\n      <div className=\"container my-3 py-3\">\n        <div className=\"row\">\n          <div className=\"col-12\">\n            <h2 className=\"display-5 text-center\">Latest Products</h2>\n            <hr />\n          </div>\n        </div>\n        <div className=\"row justify-content-center\">\n          {loading ? <Loading /> : <ShowProducts />}\n        </div>\n      </div>\n    </>\n  );\n};\n\nexport default Products;\n","/home/addiarch/Downloads/React_E-Commerce/src/components/Footer.jsx",[],[],"/home/addiarch/Downloads/React_E-Commerce/src/firebase.jsx",[],[],{"ruleId":"123","severity":1,"message":"124","line":26,"column":17,"nodeType":"125","messageId":"126","endLine":26,"endColumn":21},{"ruleId":"123","severity":1,"message":"127","line":30,"column":17,"nodeType":"125","messageId":"126","endLine":30,"endColumn":26},{"ruleId":"123","severity":1,"message":"128","line":31,"column":17,"nodeType":"125","messageId":"126","endLine":31,"endColumn":29},{"ruleId":"123","severity":1,"message":"129","line":9,"column":9,"nodeType":"125","messageId":"126","endLine":9,"endColumn":15},{"ruleId":"130","severity":1,"message":"131","line":16,"column":13,"nodeType":"132","messageId":"133","endLine":16,"endColumn":19},{"ruleId":"130","severity":1,"message":"131","line":25